#include <iostream>
using namespace std;
struct Item {
    int id;
    float cost;
};
int main() {
    Item items[6] = {
        {100, 55.5},  
        {105, 60.0},
        {40, 40.5},
        {30, 70.0},
        {0, 0.0},
        {0, 0.0}
    };
    for (int i = 0; i < 6; i++) {
        if (items[i].cost > 50) {  
            cout << "Item ID: " << items[i].id << ", Cost: " << items[i].cost << endl;
        }
    }
    return 0;
}

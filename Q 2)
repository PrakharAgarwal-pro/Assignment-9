# include <iostream>
using namespace std;

struct Colour{
    int red;
    int blue;
    int green;
};

int main () {
    Colour colour[5] = {
        colour[0] = {100, 255, 30},
        colour[1] = {243, 46, 10},
        colour[2] = {223, 56, 73},
        colour[3] = {23,175,254},
        colour[4] = {10,110, 190},
    };
    for(int i = 0; i<3; i++){
        colour[i].red=255-colour[i].red;
        colour[i].blue=255-colour[i].blue;
        colour[i].green=255-colour[i].green;
    }   
    for(int i=0; i<5; i++){
        cout<<"Red : " <<  colour[i].red << endl;
        cout<<"Blue : "  << colour[i].blue << endl;
        cout<<"green : "  << colour[i].green << endl;
    }
    return 0;
}

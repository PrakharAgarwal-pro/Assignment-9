# include <iostream>
using namespace std;

struct Point {
    float x;
    float y;
};

int main () {
    Point point [7];
    int count = 0;
    for(int i = 0; i<7; i++ ){
        cout<< "Enter the value of 7 coordinates"<<i+1<<endl;
        cin >> point[i].x >> point[i].y;
    };
    
    for(int i=0 ; i<7 ; i++){
        if(point[i].x>0 && point[i].y>0){
            count ++;}
    };
    cout<<"No. of elements in 1st quadrant : "<<count;
    return 0;
}
